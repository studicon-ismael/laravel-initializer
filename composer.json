{
    "name": "mad-web/laravel-initializer",
    "description": "Package provides ability to setup application initialization actions",
    "type": "library",
    "keywords": [
        "mad-web",
        "laravel-initializer",
        "install",
        "update",
        "deploy",
        "init",
        "initialize",
        "exec",
        "command",
        "tasks",
        "process"
    ],
    "homepage": "https://github.com/mad-web/laravel-initializer",
    "license": "MIT",
    "authors": [
        {
            "name": "Mad Web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/config": "^8.0",
        "illuminate/bus": "^8.0",
        "symfony/process": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\Initializer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\Initializer\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadWeb\\Initializer\\InitializerServiceProvider"
            ]
        }
    }
}
